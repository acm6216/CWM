<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LobsterPicker">
        <attr name="color_wheel_thickness" format="dimension" />
        <attr name="color_wheel_radius" format="dimension" />
        <attr name="color_wheel_pointer_radius" format="dimension" />
        <attr name="color_wheel_pointer_shadow_radius" format="dimension"/>
        <attr name="color_wheel_pointer_shadow" format="color"/>
        <attr name="color_wheel_scheme" format="reference" />
        <attr name="color_history_enabled" format="boolean"/>
        <attr name="color_history_radius" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="LobsterSlider">
        <attr name="color_slider_thickness" format="dimension" />
        <attr name="color_slider_length" format="dimension" />
        <attr name="color_slider_pointer_radius" format="dimension" />
        <attr name="color_slider_pointer_shadow_radius" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="LobsterShadeSlider">
        <attr name="color_slider_scheme" format="reference"/>
    </declare-styleable>
</resources>